{
  "name": "schlosswochen-ch",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "netlify": "NODE_OPTIONS=--inspect netlify functions:serve",
    "config-develop": "ts-node ./scripts/setenv.ts --environment=develop",
    "config-prod": "ts-node ./scripts/setenv.ts --environment=prod",
    "start": "npm run config-develop --environment=develop && ng serve --configuration=custom",
    "build": "npm run config-prod --environment=prod && ng build --configuration=custom",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run schlosswochen-ch:serve-ssr",
    "serve:ssr": "node dist/schlosswochen-ch/server/main.js",
    "build:ssr": "ng build && ng run schlosswochen-ch:server",
    "prerender": "ng run schlosswochen-ch:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.5",
    "@angular/google-maps": "^17.0.2",
    "@angular/material": "^17.0.2",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/platform-server": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@angular/ssr": "^17.0.5",
    "@netlify/functions": "^1.4.0",
    "axios": "^0.27.2",
    "express": "^4.18.2",
    "ngx-markdown": "^17.1.1",
    "rxjs": "~7.4.0",
    "swiper": "^11.0.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/localize": "^17.0.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.9",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.0.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "netlify-cli": "^16.9.3",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.32",
    "prettier": "2.6.2",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.8.0",
    "typescript": "~5.2.2"
  }
}
